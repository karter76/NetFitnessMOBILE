<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#435260" />
            <gradient  android:angle="-90"  android:startColor="#596d7f" android:endColor="#93b4d2"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#36797f" />
            <solid  android:color="#308d9e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#435260" />
            <gradient  android:angle="-90"  android:startColor="#93b4d2" android:endColor="#596d7f" />
        </shape>
    </item>
</selector>